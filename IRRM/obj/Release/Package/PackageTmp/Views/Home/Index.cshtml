                                            <option value="">Select</option>
                                            <option value="pie">Pie</option>
                                            <option value="doughnut">Doughnut</option>
                                            <option value="bar">Bar</option>
                                            <option value="line">Line</option>

                                        </select>
                                    </div>
                                    <div class="control-label"><b>&nbsp;&nbsp;</b></div>
                                    <div class="col-md-1">
                                        <input type="submit" class="btn btn-danger" value="GENERATE" />
                                    </div>
                                    <div class="col-md-2">
                                        <a class="btn btn-warning" href="@Url.Content("~/")">CLEAR</a>
                                    </div>
                                </form>
                            </div>

                            <script>
                                function setgraphtype() {
                                    if ($("#graphtype").val() != '') {
                                        window.location.href = '../Dashboard/' + $("#graphtype").val();
                                    }
                                }
                                $('#monthwise').hide();
                                $('#rdodaterange').click(function () {
                                    $('#monthwise').hide();
                                    $('#daterange').show();
                                });
                                $('#rdomonthwise').click(function () {
                                    $('#daterange').hide();
                                    $('#monthwise').show();
                                });


                                function getincidentsubtypes() {
                                    $.ajax({
                                        method: "GET",
                                        url: "@Url.Content("~/GetReportItems/")" + $('#reporttype').val(),
                                        contentType: false,
                                        cache: false,
                                        processData: false,
                                        success: function (data) {
                                            $("#DetailID").children('option:not(:first)').remove();
                                            //  $("#LocationID").append("<option value='' >Select</option>");
                                            $.each(data, function (key, value) {
                                                $("#DetailID").append("<option value='" + data[key].ID + "' >" + data[key].Label + "</option>");
                                            });

                                        },
                                        error: function () {
                                        }
                                    });
                                }
                            </script>

                        </div>
                    </div>
                </div>
            </div>
        </div>




