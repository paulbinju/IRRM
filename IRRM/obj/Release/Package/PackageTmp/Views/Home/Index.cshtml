@{
    ViewBag.Title = "IRRM";

}
<script src="@Url.Content("~/assets/js/chart.js")" type="text/javascript"></script>
<div class="content-page">
    <!-- Start content -->
    <div class="content">

        <div class="container">

            <div class="row">
                <div class="col-md-12">
                    <div class="panel panel-primary">
                        <div class="panel-body">
                            <h2 class="col-md-12">Dashboard</h2>
                            <h2 class="col-md-12"></h2>
                            <h2 class="col-md-12"></h2>
                            <div class="col-md-12">
                                <input type="radio" name="reporttype" id="rdodaterange" value="1" checked />&nbsp;&nbsp;Date Range Summary&nbsp;&nbsp;&nbsp;&nbsp;<input id="rdomonthwise" type="radio" name="reporttype" value="1" />&nbsp;&nbsp;Trending
                            </div>
                            <h2 class="col-md-12"></h2>
                            <div id="daterange">
                                <form method="post" action="@Url.Content("Report")" target="_blank">
                                    <div class="col-md-2">
                                        <div class="control-label"><b>Incident Date From  </b></div>
                                        <input type="date" name="IncidentDateFrom" class="form-control" required />
                                    </div>
                                    <div class="col-md-2">
                                        <div class="control-label"><b>Incident Date To  </b></div>
                                        <input type="date" name="IncidentDateTo" class="form-control" required />
                                    </div>

                                    <div class="col-md-2">
                                        <div class="control-label"><b>Report Type</b></div>
                                        <select name="reporttype" class="form-control" required>
                                            <option value="0">Select</option>
                                            <option value="1">Event Type</option>
                                            <option value="2">Incident Type</option>
                                            <option value="3">Incident Subtype</option>
                                            <option value="4">Incident Status</option>
                                            <option value="5">Harm Score</option>
                                            <option value="6">Priority</option>
                                            <option value="7">Risk Status</option>
                                            <option value="8">Risk Level</option>
                                            <option value="9">Location</option>
                                            <option value="10">Feedback</option>
                                            <option value="12">Risk Category</option>
                                            <option value="13">Risk Rating</option>
                                            <option value="14">Risk Strategy</option>
                                            <option value="15">Risk Owner</option>
                                            <option value="16">Risk Adequacy</option>
                                        </select>
                                    </div>

                                    <div class="col-md-2">
                                        <div class="control-label"><b>Graph Type</b></div>
                                        <select name="graphtype" class="form-control" required>
                                            <option value="">Select</option>
                                            <option value="pie">Pie</option>
                                            <option value="doughnut">Doughnut</option>
                                            <option value="bar">Bar</option>
                                            <option value="line">Line</option>

                                        </select>
                                    </div>
                                    <div class="control-label"><b>&nbsp;&nbsp;</b></div>
                                    <div class="col-md-1">
                                        <input type="submit" class="btn btn-danger" value="GENERATE" />
                                    </div>
                                    <div class="col-md-2">
                                        <a class="btn btn-warning" href="@Url.Content("~/")">CLEAR</a>
                                    </div>
                                </form>
                            </div>
                            <div id="monthwise">
                                <form method="post" action="@Url.Content("~/Home/ReportTrending")" target="_blank">
                                    <div class="col-md-1">
                                        <div class="control-label"><b>Month From</b></div>
                                        <select name="monthfrom" class="form-control" required>
                                            <option value="">Select</option>
                                            <option value="01">January</option>
                                            <option value="02">February</option>
                                            <option value="03">March</option>
                                            <option value="04">April</option>
                                            <option value="05">May</option>
                                            <option value="06">June</option>
                                            <option value="07">July</option>
                                            <option value="08">August</option>
                                            <option value="09">September</option>
                                            <option value="10">October</option>
                                            <option value="11">November</option>
                                            <option value="12">December</option>
                                        </select>
                                    </div>
                                    <div class="col-md-1">
                                        <div class="control-label"><b>Year From</b></div>
                                        <select name="yearfrom" class="form-control" required>
                                            <option value="">Select</option>
                                            @for (int y = ViewBag.thisyear; y >= 2017; y--)
                                            {
                                                <option value="@y">@y</option>
                                            }
                                        </select>
                                    </div>
                                    <div class="col-md-1">
                                        <div class="control-label"><b>Month To</b></div>
                                        <select name="monthto" class="form-control" required>
                                            <option value="">Select</option>
                                            <option value="01">January</option>
                                            <option value="02">February</option>
                                            <option value="03">March</option>
                                            <option value="04">April</option>
                                            <option value="05">May</option>
                                            <option value="06">June</option>
                                            <option value="07">July</option>
                                            <option value="08">August</option>
                                            <option value="09">September</option>
                                            <option value="10">October</option>
                                            <option value="11">November</option>
                                            <option value="12">December</option>
                                        </select>
                                    </div>
                                    <div class="col-md-1">
                                        <div class="control-label"><b>Year To</b></div>
                                        <select name="yearto" class="form-control" required>
                                            <option value="">Select</option>
                                            @for (int y = ViewBag.thisyear; y >= 2017; y--)
                                            {
                                                <option value="@y">@y</option>
                                            }
                                        </select>
                                    </div>

                                    <div class="col-md-2">
                                        <div class="control-label"><b>Report Type</b></div>
                                        <select name="reporttype" id="reporttype" class="form-control" required  onchange="getincidentsubtypes()">
                                            <option value="">Select</option>
                                            <option value="1">Event Type</option>
                                            <option value="2">Incident Type</option>
                                            <option value="3">Incident Subtype</option>
                                            <option value="4">Incident Status</option>
                                            <option value="5">Harm Score</option>
                                            <option value="6">Priority</option>
                                            <option value="7">Risk Status</option>
                                            @*<option value="8">Risk Level</option>*@
                                            <option value="9">Location</option>
                                            <option value="10">Feedback</option>
                                            <option value="12">Risk Category</option>
                                            @*<option value="13">Risk Rating</option>*@
                                            <option value="14">Risk Strategy</option>
                                            <option value="15">Risk Owner</option>
                                            <option value="16">Risk Adequacy</option>
                                        </select>
                                    </div>
 
                                    <div class="col-md-2">
                                        <div class="control-label"><b>Details</b></div>
                                        <select name="DetailID" id="DetailID" class="form-control" required>
                                            <option value="0">Select All</option>
                                        </select>
                                    </div>

                                    <div class="col-md-1">
