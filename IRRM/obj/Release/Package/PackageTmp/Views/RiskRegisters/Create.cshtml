                                                            }
                                                            else if (Impactval == 25) {
                                                                jQuery("#RARiskLevel").val('Extreme Risk');
                                                                getrisklevel('Extreme Risk');
                                                            }

                                                        }


                                                        function getrisklevel(level) {
                                                            jQuery.ajax({
                                                                method: "GET",
                                                                url: "@Url.Content("~/GetRiskLevel/")" + level,
                                                                contentType: false,
                                                                cache: false,
                                                                processData: false,
