                                                        function calImpact(Impactval) {

                                                            jQuery("#RARating").val(Impactval);

                                                            if (Impactval <= 9) {
                                                                jQuery("#RARiskLevel").val('Low Risk');
                                                                getrisklevel('Low Risk');
                                                            }
                                                            else if (Impactval >= 10 && Impactval <= 15) {
